[project]
	description = Distributed machine learning platform
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group Authorities
[access "refs/for/refs/*"]
	read = group Authorities
	push = group Authorities
	label-Code-Review = -2..+2 group Authorities
	label-Verified = -1..+1 group Authorities
	rebase = group Authorities
	removeReviewer = group Authorities
	submit = group Authorities
[access "refs/heads/*"]
	read = group Authorities
	create = group Authorities
	push = +force group Authorities
	pushMerge = group Authorities
[access "refs/tags/*"]
	push = +force group Authorities
	pushTag = group Authorities
	pushSignedTag = group Authorities
